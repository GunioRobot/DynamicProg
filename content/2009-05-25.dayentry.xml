&lt;/pre&gt;&lt;pre style="background-color: #fbfbfb; margin: 0em; width: 100%; font-family: consolas,&amp;#39;Courier New&amp;#39;,courier,monospace; font-size: 12px"&gt; 17:         &lt;span style="color: #0000ff"&gt;public&lt;/span&gt; T4 Item4{&lt;span style="color: #0000ff"&gt;get&lt;/span&gt;;&lt;span style="color: #0000ff"&gt;private&lt;/span&gt; &lt;span style="color: #0000ff"&gt;set&lt;/span&gt;;}
